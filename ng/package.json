{
  "name": "interview-multiplayer-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "node --max-old-space-size=6144 ./node_modules/@angular/cli/bin/ng",
    "start:base": "npm run ng -- serve --port=4300",
    "start:dev": "npm run start:base -- -c dev --base-href /app/dev/ --proxy-config ./config/proxy/proxy-dev.config.js",
    "start:mock": "npm run start:base -- -c mock --base-href /app/mock/ --proxy-config ./config/proxy/proxy-mock.config.js",
    "start:prod": "npm run start:base -- -c prod --base-href /app/prod/ --proxy-config ./config/proxy/proxy-prod.config.js",
    "start:prod-develop": "npm run start:base -- -c prod-develop --base-href /app/prod-develop/ --proxy-config ./config/proxy/proxy-prod.config.js",
    "build:base": "npm run ng -- build",
    "build:prod-develop": "npm run build:base -- -c prod-develop --base-href /app/index.html/ --deploy-url /app/",
    "build:prod": "npm run build:base -- -c prod --base-href /app/index.html/ --deploy-url /app/",
    "test": "ng test",
    "lint": "ng lint && sass-lint -v -q",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.1.1",
    "@angular/material-moment-adapter": "^7.1.1",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@ngrx/effects": "^7.0.0-beta.1",
    "@ngrx/router-store": "^7.0.0-beta.1",
    "@ngrx/store": "^7.0.0-beta.1",
    "@ngrx/store-devtools": "^7.0.0-beta.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "moment": "^2.23.0",
    "ngrx-store-freeze": "^0.2.4",
    "rxjs": "~6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.4.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "sass-lint": "^1.12.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
